cmap: CONTACT_MATRIX ...
   GROUP=1-201
   SWITCH={RATIONAL D_0=1.0 R_0=0.5 D_MAX=2.0}
   COMPONENTS
...

# Now construct histogram
cmapn: NORMALIZE ARG1=cmap.x ARG2=cmap.y ARG3=cmap.z 
h: SPHERICAL_KDE ARG1=cmapn.x ARG2=cmapn.y ARG3=cmapn.z HEIGHTS=cmap.w CONCENTRATION=10 GRID_BIN=100
iv: INTEGRATE_GRID ARG=h PERIODIC=NO

b: RESTRAINT ARG=iv AT=0.75 KAPPA=10.0
PRINT ARG=iv,b.* FILE=colvar FMT=%8.4f
